{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ProcessingConfig": {
    "PathsProcessing": [
      {
        "regexp": "^(?<monitoring_module>[^\/]*)\/(?<mr>[^\/]*)\/(?<system_name>[^\/]*)\/(?<server_name>[^\/]*)\/(?<template_name>[^\/]*)\/(?<instance_name>[^\/]*)\/([^\/]*)$"
      },
      {
        "regexp": "^(?<monitoring_module>[^\/]*)\/(?<mr>[^\/]*)\/(?<system_name>[^\/]*)\/(?<server_name>[^\/]*)\/(?<template_name>[^\/]*)\/(?<instance_name>)([^\/]*)$",
        "defaults": "instance_name=[def]"
      }
    ],
    "CounterProcessing": [
      {
        "monitorName": "^JSON_CustomAttr$",
        "counterName": "^metrics$",
        "valueRegexp": "(?<name>[^=]+)=(?<value>[^,]+)"
      },
      {
        "monitorName": "^OS_CPUUtil$",
        "counterName": "^(?!.*( cpu )).*$",
        "valueRegexp": ".*"
      },
      {
        "monitorName": "^(OS_FSMountStatus|OS_ProcMemUtil|OS_ProcStatus|Win_CustomCounter|Calls_Duration|Calls_Outstanding|Calls_Failed_Per_Second|Calls_Faulted_Per_Second|OS_ProcCPUUtil)\\w+",
        "counterName": "\\\\\\\\(?<__name__>[^\\\\]+)$"
      },
      {
        "monitorName": "^(VMware_|JSON_Metric|DB_Ora_Inst_Tblspc_PctUsed).*",
        "counterName": "/(?<__name__>[^/]+)$"
      },
      {
        "monitorName": "^OS_WinDiskSpace\\w+",
        "counterName": "(?<__name__>[^/]+\/[^/]+$)"
      }

    ]
  }
}
